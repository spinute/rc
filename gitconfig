[user]
	name = spinute
[color]
	ui = auto
[core]
	editor = vim
[alias]
	co = checkout
	st = status
	br = branch
	ci = commit

	# cp from http://qiita.com/peccul/items/90dd469e2f72babbc106
	graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
	gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'

	al = add .
	com = checkout master
	std = diff --staged
	unstage = reset HEAD --
	last = log -1 HEAD
[push]
	default = matching
